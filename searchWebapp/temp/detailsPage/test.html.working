<!DOCTYPE html>
<head>
    <script src="http://code.jquery.com/jquery.min.js" type="text/javascript" charset="utf-8"></script>
    <script src="./CDMI.js" type="text/javascript" charset="utf-8"></script>
    <script>
    $(function () {
        var api = new CDMI_EntityAPI('http://kbase.us/services/cdmi_api');
        api.all_entities_Genome_async(0, 100, ['source_id', 'scientific_name'], function (genomes) {
            $("body").append($("<table>")
                .append($("<tbody>").attr("id", "table-body")));
            for (var id in genomes) {
                $("#table-body").append($("<tr>")
                    .append($("<th>").text(id))
                    .append($("<td>").text(genomes[id].scientific_name)));
            }
        });
    })
    </script>
</head>
<body>
</body>
