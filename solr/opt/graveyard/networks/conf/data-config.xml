<dataConfig>
  <dataSource type="JdbcDataSource" driver="com.mysql.jdbc.Driver" url="jdbc:mysql://db1.chicago.kbase.us/networks_pdev"  user="networks_pdev"  />
  <document>
    <entity name="pathways" query="select * from gene_pathway_int, pathway_name where gene_pathway_int.pathway = pathway_name.pathway" transformer="TemplateTransformer">
        <field column='SName' name='sname' />
        <field column='gene' name='gid' />
        <field column='pathway' name='pathway' />
        <field column='kbfid' name='kbfid' />
        <field column='name' name='name' />
        <field column='id' name='id' template="${pathways.kbfid}_${pathways.pathway}" /> 
    </entity>
  </document>
</dataConfig>
