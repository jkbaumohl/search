<dataConfig>
  <dataSource type="JdbcDataSource" driver="com.mysql.jdbc.Driver" url="jdbc:mysql://db1.chicago.kbase.us/networks_pdev"  user="networks_pdev"  />
  <document>
    <entity name="ontologies" query="select * from ontologies_int" transformer="TemplateTransformer">
        <field column='SName' name='sname' />
        <field column='TranscriptID' name='trid' />
        <field column='OntologyID' name='oid' />
        <field column='OntologyDescription' name='odesc' />
        <field column='OntologyDomain' name='domain' />
        <field column='OntologyEvidenceCode' name='ec' />
        <field column='OntologyType' name='type' />
        <field column='GeneID' name='gid' />
        <field column='kbtrid' name='kbtrid' />
        <field column='kblocusid' name='kblcid' />
        <field column='id' name='id' template="${ontologies.kbtrid}_${ontologies.OntologyID}_${ontologies.OntologyEvidenceCode}" /> 
    </entity>
  </document>
</dataConfig>
